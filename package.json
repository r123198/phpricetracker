{
  "name": "ph-price-tracker-api",
  "version": "1.0.0",
  "description": "Philippine Market Price Tracker REST API with automated scrapers",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest tests/api.test.js",
    "test:scrapers": "jest tests/scrapers.test.js",
    "test:utils": "jest tests/utils.test.js",
    "test:security": "jest tests/security/",
    "test:unit": "jest --testPathPattern=tests/.*\\.test\\.js --testNamePattern='(API|Scrapers|Utils)'",
    "test:integration": "jest --testPathPattern=tests/api.test.js --testNamePattern='Integration'",
    "scrape": "node scrapers/run_all.js",
    "scrape:da": "node scrapers/pdf/da_parser.js",
    "scrape:doe": "node scrapers/pdf/doe_parser.js",
    "scrape:dti": "node scrapers/pdf/dti_parser.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node src/database/seed.js",
    "db:studio": "prisma studio",
    "setup": "node setup.js",
    "setup:docker": "node setup-docker.js",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f postgres",
    "docker:reset": "docker compose down -v && docker compose up -d",
    "db:setup": "npm run docker:up && sleep 5 && npm run db:generate && npm run db:push && npm run db:seed",
    "build": "docker build -t ph-price-tracker .",
    "docker:run": "docker run -p 3000:3000 --env-file .env ph-price-tracker",
    "deploy:digitalocean": "chmod +x scripts/deploy-digitalocean.sh && ./scripts/deploy-digitalocean.sh",
    "deploy:docker": "chmod +x scripts/deploy-docker.sh && ./scripts/deploy-docker.sh",
    "deploy:docker:down": "docker-compose -f docker-compose.production.yml down",
    "deploy:docker:logs": "docker-compose -f docker-compose.production.yml logs -f",
    "deploy:docker:restart": "docker-compose -f docker-compose.production.yml restart",
    "deploy:railway": "railway up",
    "deploy:render": "render deploy",
    "deploy:heroku": "git push heroku main",
    "build:production": "docker build -f Dockerfile.production -t ph-price-tracker:production .",
    "pm2:start": "pm2 start index.js --name ph-price-tracker",
    "pm2:stop": "pm2 stop ph-price-tracker",
    "pm2:restart": "pm2 restart ph-price-tracker",
    "pm2:logs": "pm2 logs ph-price-tracker",
    "pm2:monit": "pm2 monit",
    "pm2:delete": "pm2 delete ph-price-tracker",
    "ssl:certbot": "sudo certbot --nginx -d yourdomain.com",
    "ssl:renew": "sudo certbot renew --quiet",
    "backup:db": "pg_dump $DATABASE_URL > backup_$(date +%Y%m%d_%H%M%S).sql",
    "restore:db": "psql $DATABASE_URL < backup_file.sql",
    "setup:env": "chmod +x scripts/setup-env.sh && ./scripts/setup-env.sh"
  },
  "keywords": [
    "philippines",
    "price-tracker",
    "commodities",
    "api",
    "scraping"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bullmq": "^5.1.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "pdf-parse": "^1.1.1",
    "prisma": "^5.7.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
